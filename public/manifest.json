{
    "manifest_version": 3,
    "name": "Tab Habit Tracker",
    "description": "Track, categorize, and manage your browser tabs effectively.",
    "version" : "0.1",
    "icons": {
        "48": "image.png",
        "128": "image.png"
    },
    "action": {
        "default_title": "Tab Tracker",
        "default_icon": {
            "16": "image.png",
            "24": "image.png",
            "32": "image.png"
        },
        "default_popup": "index.html"
        },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage",
                    "tabs",
                    "notifications"
                ],
    "host_permissions": ["http://*/*", "https://*/*"]
}
